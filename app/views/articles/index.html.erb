<div class="container">
  <h1 class="display-4 py-4">KNYTimes</h1>
  <div class="row">
    <% for article in @articles %>
      <div class="col-sm-3 col-md-4 mb-5">
        <a href="<%= article["url"] %>" target="_blank">
          <img src="<%= article["multimedia"][1]["url"] %>" class="img-thumbnail mb-3">
          <%= article["title"] %>
        </a>
      </div>
    <% end %>
  </div>
</div>